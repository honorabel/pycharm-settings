<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" pattern="(todo).*\(abel\).*" />
    <pattern icon="question" useCustomColors="true" pattern="(note).*\(abel\).*">
      <option name="FOREGROUND" value="1dbb12" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="108d04" />
    </pattern>
    <filter name="Abel TODO's">
      <pattern index="2" />
      <pattern index="3" />
    </filter>
  </component>
</application>