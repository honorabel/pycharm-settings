<application>
  <component name="CvsApplicationLevelConfiguration">
    <option name="PATH_TO_PASSWORD_FILE" value="$userdir/.cvspass" />
  </component>
  <component name="PyCodeInsightSettings">
    <option name="SHOW_IMPORT_POPUP" value="true" />
  </component>
  <component name="PyTestFrameworkService">
    <option name="SDK_TO_PYTEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/versions/hpy-external-api/bin/python" value="true" />
        <entry key="$USER_HOME$/.pyenv/versions/hpy-workflow/bin/python" value="false" />
      </map>
    </option>
    <option name="SDK_TO_NOSETEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/versions/hpy-external-api/bin/python" value="true" />
        <entry key="$USER_HOME$/.pyenv/versions/hpy-workflow/bin/python" value="true" />
      </map>
    </option>
    <option name="SDK_TO_ATTEST">
      <map>
        <entry key="$USER_HOME$/.pyenv/versions/hpy-external-api/bin/python" value="false" />
        <entry key="$USER_HOME$/.pyenv/versions/hpy-workflow/bin/python" value="false" />
      </map>
    </option>
  </component>
</application>